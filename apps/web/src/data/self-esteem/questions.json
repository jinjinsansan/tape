    { "id": "P01", "category": "P", "type": "P", "text": "自分は幸せになっていいと思う" },
    { "id": "P02", "category": "P", "type": "P", "text": "欲しいものを欲しいと言っていいと思う" },
    { "id": "P03", "category": "P", "type": "P", "text": "自分の望みを大切にしていいと感じる" },
    { "id": "P04", "category": "P", "type": "P", "text": "休んでも、自分を責めない" },
    { "id": "P05", "category": "P", "type": "P", "text": "自分のために時間やお金を使っても罪悪感がない" },
    { "id": "P06", "category": "P", "type": "P", "text": "助けを求めてもいいと思える" },
    { "id": "P07", "category": "P", "type": "P", "text": "楽しむことに罪悪感がない" },
    { "id": "P08", "category": "P", "type": "P", "text": "自分の気持ちを優先してもいいと思える" },
    { "id": "P09", "category": "P", "type": "P", "text": "「ノー」と言ってもいいと思う" },
    { "id": "P10", "category": "P", "type": "P", "text": "自分を大切にしてもいいと思える" },
    { "id": "P11", "category": "P", "type": "P", "text": "成功していいと自分に許可できる" },
    { "id": "P12", "category": "P", "type": "P", "text": "褒められたら、素直に受け取れる" },
    { "id": "P13", "category": "P", "type": "P", "text": "自分のために生きてもいいと思う" },
    { "id": "P14", "category": "P", "type": "P", "text": "贅沢をしても、罪悪感がない" },
    { "id": "P15", "category": "P", "type": "P", "text": "自分の時間を持っていいと思える" },
    { "id": "P16", "category": "P", "type": "P", "text": "人に頼ってもいいと思う" },
    { "id": "P17", "category": "P", "type": "P", "text": "疲れたら休んでいいと思える" },
    { "id": "P18", "category": "P", "type": "P", "text": "自分の感情を感じてもいいと思う" },
    { "id": "P19", "category": "P", "type": "P", "text": "怒ってもいいと自分に許可できる" },
    { "id": "P20", "category": "P", "type": "P", "text": "泣いてもいいと思える" },
    { "id": "P21", "category": "P", "type": "P", "text": "自分のペースで進んでいいと思う" },
    { "id": "P22", "category": "P", "type": "P", "text": "完璧でなくてもいいと思える" },
    { "id": "P23", "category": "P", "type": "P", "text": "失敗してもいいと自分に許可できる" },
    { "id": "P24", "category": "P", "type": "P", "text": "自分らしくいていいと思える" },
    { "id": "P25", "category": "P", "type": "N", "text": "自分だけ幸せになるのは申し訳ないと感じる" },
    { "id": "P26", "category": "P", "type": "N", "text": "欲しいものを欲しがるのは、わがままだと思う" },
    { "id": "P27", "category": "P", "type": "N", "text": "自分の望みより、人の望みを優先すべきだと感じる" },
    { "id": "P28", "category": "P", "type": "N", "text": "休むことに罪悪感がある" },
    { "id": "P29", "category": "P", "type": "N", "text": "自分のためにお金を使うのは贅沢だと感じる" },
    { "id": "P30", "category": "P", "type": "N", "text": "人に迷惑をかけてはいけないと強く思う" },
    { "id": "P31", "category": "P", "type": "N", "text": "楽しむと、後で罰が当たると思ってしまう" },
    { "id": "P32", "category": "P", "type": "N", "text": "自分を後回しにするのが当然だと感じる" },
    { "id": "P33", "category": "P", "type": "N", "text": "断ることに強い罪悪感がある" },
    { "id": "P34", "category": "P", "type": "N", "text": "自分を大切にするのは、自己中心的だと思う" },
    { "id": "P35", "category": "P", "type": "N", "text": "成功すると、何か悪いことが起きそうで怖い" },
    { "id": "P36", "category": "P", "type": "N", "text": "褒められても、素直に喜べない" },
    { "id": "P37", "category": "P", "type": "N", "text": "自分のために生きるのは、わがままだと思う" },
    { "id": "P38", "category": "P", "type": "N", "text": "贅沢をすると、罪悪感で苦しくなる" },
    { "id": "P39", "category": "P", "type": "N", "text": "自分の時間を持つのは申し訳ないと感じる" },
    { "id": "P40", "category": "P", "type": "N", "text": "人に頼るのは、弱さだと思う" },
    { "id": "B01", "category": "B", "type": "P", "text": "私は私、あなたはあなただと思える" },
    { "id": "B02", "category": "B", "type": "P", "text": "嫌なことは嫌だとはっきり言える" },
    { "id": "B03", "category": "B", "type": "P", "text": "人の問題と自分の問題を分けて考えられる" },
    { "id": "B04", "category": "B", "type": "P", "text": "自分の意見をはっきり持っている" },
    { "id": "B05", "category": "B", "type": "P", "text": "自分が何を感じているか分かる" },
    { "id": "B06", "category": "B", "type": "P", "text": "人との距離感を自分で決められる" },
    { "id": "B07", "category": "B", "type": "P", "text": "頼まれても、無理なことは断れる" },
    { "id": "B08", "category": "B", "type": "P", "text": "相手の気分に振り回されない" },
    { "id": "B09", "category": "B", "type": "P", "text": "自分の考えと相手の考えを区別できる" },
    { "id": "B10", "category": "B", "type": "P", "text": "私には私の人生があると思える" },
    { "id": "B11", "category": "B", "type": "P", "text": "人の期待に応えなくても、自分でいられる" },
    { "id": "B12", "category": "B", "type": "P", "text": "誰かといても、自分を見失わない" },
    { "id": "B13", "category": "B", "type": "P", "text": "自分の気持ちを大切にできる" },
    { "id": "B14", "category": "B", "type": "P", "text": "相手がどう思うかより、自分がどう思うかを優先できる" },
    { "id": "B15", "category": "B", "type": "P", "text": "人の意見に流されずに判断できる" },
    { "id": "B16", "category": "B", "type": "P", "text": "自分の領域に踏み込まれたら、嫌だと言える" },
    { "id": "B17", "category": "B", "type": "P", "text": "相手の問題は相手が解決すべきだと思える" },
    { "id": "B18", "category": "B", "type": "P", "text": "自分の時間や空間を守ることができる" },
    { "id": "B19", "category": "B", "type": "P", "text": "人に合わせすぎずに、自分らしくいられる" },
    { "id": "B20", "category": "B", "type": "P", "text": "「ここまでは手伝うけど、ここからは無理」と線引きできる" },
    { "id": "B21", "category": "B", "type": "P", "text": "自分がどうしたいかが分かる" },
    { "id": "B22", "category": "B", "type": "P", "text": "相手の感情は相手のものだと思える" },
    { "id": "B23", "category": "B", "type": "P", "text": "人といても、自分の存在を感じられる" },
    { "id": "B24", "category": "B", "type": "P", "text": "自分の幸せを優先しても罪悪感がない" },
    { "id": "B25", "category": "B", "type": "N", "text": "誰かの話を聞いていると、自分のことのように苦しくなる" },
    { "id": "B26", "category": "B", "type": "N", "text": "断ると相手に悪いと思って、つい引き受けてしまう" },
    { "id": "B27", "category": "B", "type": "N", "text": "自分の気持ちより相手の気持ちを優先してしまう" },
    { "id": "B28", "category": "B", "type": "N", "text": "相手が怒っていると、自分が悪いと感じてしまう" },
    { "id": "B29", "category": "B", "type": "N", "text": "人の問題を自分が何とかしなければと思う" },
    { "id": "B30", "category": "B", "type": "N", "text": "自分がどんどん小さくなっていく感覚がある" },
    { "id": "B31", "category": "B", "type": "N", "text": "私という存在が消えていく感じがすることがある" },
    { "id": "B32", "category": "B", "type": "N", "text": "人にずけずけと踏み込まれてしまう" },
    { "id": "B33", "category": "B", "type": "N", "text": "相手の機嫌が悪いと、自分のせいだと感じる" },
    { "id": "B34", "category": "B", "type": "N", "text": "嫌われるのが怖くて、自分の意見を言えない" },
    { "id": "B35", "category": "B", "type": "N", "text": "自分が何を感じているのか分からなくなることがある" },
    { "id": "B36", "category": "B", "type": "N", "text": "相手に合わせすぎて、自分がなくなる感じがする" },
    { "id": "B37", "category": "B", "type": "N", "text": "誰かと一緒にいると、自分を見失う" },
    { "id": "B38", "category": "B", "type": "N", "text": "ニュースや他人の話で、必要以上に苦しくなる" },
    { "id": "B39", "category": "B", "type": "N", "text": "人の期待に応えようとして、自分を犠牲にしてしまう" },
    { "id": "B40", "category": "B", "type": "N", "text": "自分と他人の境目が分からなくなることがある" }
  ]
}
